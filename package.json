{
  "name": "fibonachicken",
  "version": "0.9.2",
  "main": "expo-router/entry",
  "scripts": {
    "format": "biome check --write .",
    "start": "doppler run -- expo start",
    "android": "doppler run -- expo start --android",
    "ios": "doppler run -- expo start --ios",
    "web": "doppler run -- expo start --web",
    "insert-manifest": "awk 'NR==5 {print \"    <link rel=\\\"manifest\\\" href=\\\"/manifest.json\\\">\"} 1' dist/index.html > temp && mv temp dist/index.html",
    "build:web:ci": "expo export -p web && npm run insert-manifest",
    "build:web": "doppler run -- npm run build:web:ci"
  },
  "dependencies": {
    "@biomejs/biome": "^1.9.4",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@shopify/flash-list": "2.0.2",
    "@types/react": "~19.1.10",
    "expo": "~54.0.12",
    "expo-application": "~7.0.7",
    "expo-constants": "~18.0.9",
    "expo-device": "~8.0.9",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.12",
    "jotai": "^2.15.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
