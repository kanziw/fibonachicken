{
  "cli": {
    "version": ">= 7.8.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "20.12.1",
      "autoIncrement": true,
      "android": {
        "image": "latest"
      },
      "ios": {
        "image": "latest"
      }
    },
    "production": {
      "extends": "base",
      "channel": "main"
    },
    "preview": {
      "extends": "base",
      "distribution": "internal"
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal"
    }
  },
  "submit": {
    "production": {}
  }
}
